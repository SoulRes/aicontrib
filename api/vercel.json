{
  "routes": [
    {
      "src": "/installer-windows\\.exe",
      "headers": { "Content-Disposition": "attachment" },
      "continue": true
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/.*",
      "has": [
        { "type": "header", "key": "x-vercel-ip-country", "value": "US" }
      ],
      "dest": "/api/geo-block.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
